            </trans-unit>
            <trans-unit id="mremi_contact.form.last_name">
                <source>form.contact_form.children.lastName.label</source>
                <target>Achternaam</target>
            </trans-unit>
            <trans-unit id="mremi_contact.form.company.label">
                <source>form.contact_form.children.company.label</source>
                <target>Bedrijf</target>
            </trans-unit>
            <trans-unit id="form.contact_form.children.phone.label">
                <source>form.contact_form.children.phone.label</source>
                <target>Telefoonnummer</target>
            </trans-unit>
            <trans-unit id="mremi_contact.form.children.email.label">
                <source>mremi_contact.form.email</source>
                <target>Email</target>
